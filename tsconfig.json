{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/components/ProductForm.jsx", "src/app/api/stripe/route.jsx", "src/app/(pages)/users/page.jsx", "src/app/components/UserForm.jsx", "src/app/products/page.jsx", "src/app/components/UserEdit.jsx", "src/app/components/Pagination.jsx", "src/app/components/Navbar.jsx", "src/app/components/EditForm.jsx", "src/app/components/BuyButton.jsx", "src/app/components/Box.jsx", "src/app/(pages)/users/[profile]/page.jsx", "src/app/(pages)/product_panel/page.jsx", "src/app/(pages)/cart/page.jsx", "src/app/(Engine)/actions/fetchproduct.jsx", "src/app/(Engine)/actions/fetchSearch.jsx", "src/app/layout.jsx", "src/app/components/Reset.jsx", "src/app/api/recovery/reset/[email]", "src/app/(Auth)/register/page.jsx", "src/app/page.jsx", "src/app/components/Login.jsx", "src/app/components/Uploader.jsx", "src/app/components/SearchBar.jsx", "src/app/components/Multipier.jsx", "src/app/components/Modal.jsx", "src/app/components/Card.jsx"],
  "exclude": ["node_modules"]
}
